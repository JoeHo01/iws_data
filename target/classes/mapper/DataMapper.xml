<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xj.iws.http.dao.DataDao">

    <sql id="field">deviceid,port,number,time,error,count,data</sql>

    <insert id="add" parameterType="DataEntity">
        INSERT INTO `${tableName}` (<include refid="field" />) VALUES (${deviceId},'${port}','${number}',now(),'${error}',${count},'${data}')
    </insert>

</mapper>